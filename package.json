{
  "name": "icms-backend",
  "version": "1.0.0",
  "description": "Integrated Content Management System Backend for Government CMS",
  "author": "ICMS Development Team",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:setup": "./test/run-tests.sh setup",
    "test:all": "./test/run-tests.sh all",
    "test:cleanup": "./test/run-tests.sh cleanup",
    "test:story-docs": "jest --config ./test/jest-e2e.json --testPathPatterns=story.e2e-spec.ts",
    "test:story-auth": "jest --config ./test/jest-e2e.json --testPathPatterns=auth.*story.e2e-spec.ts",
    "test:story-content": "jest --config ./test/jest-e2e.json --testPathPatterns=content.*story.e2e-spec.ts",
    "test:story-documents": "jest --config ./test/jest-e2e.json --testPathPatterns=documents.*story.e2e-spec.ts",
    "test:story-header": "jest --config ./test/jest-e2e.json --testPathPatterns=header.*story.e2e-spec.ts",
    "test:story-hr": "jest --config ./test/jest-e2e.json --testPathPatterns=hr.*story.e2e-spec.ts",
    "test:story-important-links": "jest --config ./test/jest-e2e.json --testPathPatterns=important-links.*story.e2e-spec.ts",
    "test:story-media": "jest --config ./test/jest-e2e.json --testPathPatterns=media.*story.e2e-spec.ts",
    "test:story-users": "jest --config ./test/jest-e2e.json --testPathPatterns=users.*story.e2e-spec.ts",
    "test:faq": "jest --config ./test/jest-e2e.json --testPathPatterns=faq.*story.e2e-spec.ts",
    "test:bootstrap": "./test/run-bootstrap-tests.sh all",
    "test:bootstrap:env": "./test/run-bootstrap-tests.sh env",
    "test:bootstrap:default": "./test/run-bootstrap-tests.sh default",
    "test:bootstrap:duplicates": "./test/run-bootstrap-tests.sh duplicates",
    "test:bootstrap:errors": "./test/run-bootstrap-tests.sh errors",
    "test:bootstrap:security": "./test/run-bootstrap-tests.sh security",
    "test:bootstrap:integration": "./test/run-bootstrap-tests.sh integration",
    "test:bootstrap:config": "./test/run-bootstrap-tests.sh config",
    "test:bootstrap:coverage": "./test/run-bootstrap-tests.sh coverage",
    "test:bootstrap:watch": "./test/run-bootstrap-tests.sh watch",
    "test:bootstrap:debug": "./test/run-bootstrap-tests.sh debug",
    "test:bootstrap:setup": "./test/run-bootstrap-tests.sh setup",
    "test:bootstrap:cleanup": "./test/run-bootstrap-tests.sh cleanup",
    "test:bootstrap:simple": "jest --config ./test/jest-e2e.json --testPathPatterns=bootstrap.e2e-spec.ts",
    "docs:generate": "npm run test:story-docs && echo 'Story documentation generated in test/story-docs/output/'",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:reset": "prisma migrate reset",
    "db:migrate:status": "prisma migrate status",
    "db:seed": "ts-node prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:format": "prisma format",
    "db:validate": "prisma validate",
    "db:diff": "prisma db diff",
    "db:drop": "prisma db push --force-reset",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.850.0",
    "@aws-sdk/s3-request-presigner": "^3.850.0",
    "@nestjs/common": "^11.1.5",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.5",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.5",
    "@nestjs/platform-socket.io": "^11.1.5",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/websockets": "^11.1.5",
    "@paralleldrive/cuid2": "^2.2.2",
    "@prisma/client": "^6.12.0",
    "@types/axios": "^0.14.4",
    "@types/sharp": "^0.32.0",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "compression": "^1.8.1",
    "connect-redis": "^9.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express-rate-limit": "^8.0.1",
    "express-session": "^1.18.2",
    "express-slow-down": "^2.1.0",
    "express-validator": "^7.2.1",
    "handlebars": "^4.7.8",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "minio": "^8.0.5",
    "moment": "^2.30.1",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.5",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "redis": "^5.6.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "sharp": "^0.34.3",
    "slugify": "^1.6.6",
    "socket.io": "^4.8.1",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.5",
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/moment": "^2.13.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.0.15",
    "@types/nodemailer": "^6.4.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "prisma": "^6.12.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.3",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
